{
    "git.path": "wsl",
    "remote.WSL.fileSystemWatcher": true,
    "git.detectSubmodules": true,
    "git.fetchOnPull": true,
    "git.autofetch": true,
    "git.showInlineOpenFileAction": true,
    "git.openRepositoryInParentFolders": "always",
    "git.scanRepositories": [
        ".",
        "./qicore-v4"
    ],
    "git.ignoredRepositories": [],
    "git.autoRepositoryDetection": "subFolders",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "scm.showIncomingChanges": "always",
    "scm.showOutgoingChanges": "always",
    "scm.repositories.visible": 10,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/.git": true
    },
    "workbench.colorCustomizations": {
        "gitDecoration.submoduleResourceForeground": "#888888",
        "gitDecoration.untrackedResourceForeground": "#73C991",
        "gitDecoration.modifiedResourceForeground": "#E2C08D",
        "gitDecoration.deletedResourceForeground": "#F14C4C",
        "gitDecoration.conflictingResourceForeground": "#E64946"
    },
    "terminal.integrated.defaultProfile.linux": "bash"
} 